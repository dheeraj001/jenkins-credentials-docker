pipeline {
  agent any
  parameters {
  credentials credentialType: 'com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl', defaultValue: '', name: 'DOCKER_PARAM', required: true
}
  environment {
    DOCKER = credentials("${params.DOCKER_PARAM}")
  }
  stages {
    stage('Docker Build') {
      steps {
        sh 'docker build -t dheerajlearn/jenkins-web:latest .'
      }
    }
    stage('Docker Push') {
      steps {
          sh "docker login -u ${env.DOCKER_USR} -p ${env.DOCKER_PSW}"
          sh 'docker push dheerajlearn/jenkins-web:latest'
        
      }
    }
  }
}